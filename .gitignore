# cake sessions
app/tmp/sessions/*

# Cake logs
app/tmp/logs/*.log

# Wildflower caches
app/tmp/cache/wf_previews/*.json
app/tmp/cache/wf_root_pages
app/tmp/cache/wf_thumbnails/*.jpg

# Wildflower uploaded files
app/webroot/uploads/*
#asset cache dirs
app/webroot/ccss
app/webroot/cjs

# Ruckusing log file
vendors/ruckusing/logs/development.log

# HTML webroot cache
app/webroot/cache/*

# .bak files
*.bak
